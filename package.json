{
  "name": "petfinder-site",
  "version": "1.0.0",
  "description": "Pet owners are able to easily find sitters at an affordable price and convenient time; Pet sitters can find work that first their schedules easily",
  "author": "Mario Arturo Lopez Martinez",
  "repository": "https://gitlab.com/fall18-group2/group-2",
  "license": "ISC",
  "main": "webpack.config.js",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "dev": "cross-env webpack-dev-server --env dev",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "node_modules": "rm -rf node_modules/ && rm -rf package-lock.json",
    "node_modules-w": "rmdir node_modules /s && del package-lock.json",
    "install:clean": "npm run node_modules && npm install && npm start",
    "install:clean-w": "npm run node_modules-w && npm install && npm start"
  },
  "jest": {
    "roots": [
      "<rootDir>/build/tests"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>"
    ],
    "verbose": true,
    "transform": {}
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "fingerprintjs2": "^1.5.1",
    "glob": "^7.1.2",
    "install": "^0.8.7",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "@appbaseio/reactivesearch": "2.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "4.0.0-beta.3",
    "copy-webpack-plugin": "^4.5.3",
    "font-awesome": "^4.7.0",
    "i": "^0.3.6",
    "jquery": "^3.3.1",
    "react-available-times": "^1.2.1",
    "react-hot-loader": "4.0.0-beta.14",
    "react-phone-input-2": "^2.6.1",
    "react-popper": "^1.0.2",
    "react-router": "^4.3.1",
    "react-select": "1.2.1",
    "reactstrap": "6.5.0",
    "uuid": "^3.3.2",
    "@material-ui/core": "3.1.1",
    "@material-ui/icons": "3.0.1",
    "axios": "^0.18.0",
    "chartist": "0.10.1",
    "classnames": "^2.2.6",
    "history": "4.7.2",
    "lodash": "^4.17.11",
    "moment": "2.22.2",
    "node-sass-chokidar": "1.3.3",
    "nouislider": "12.0.0",
    "npm": "^6.4.1",
    "npm-run-all": "4.1.3",
    "perfect-scrollbar": "1.4.0",
    "prop-types": "^15.6.2",
    "react": "16.6.0",
    "react-big-calendar": "0.20.1",
    "react-bootstrap-sweetalert": "4.4.1",
